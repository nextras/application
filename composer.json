{
	"name": "nextras/secured-links",
	"type": "library",
	"description": "Package secures Nette Framework signals against CSRF attack.",
	"keywords": ["nextras", "nette", "application", "secured signals"],
	"license": ["MIT"],
	"authors": [
		{
			"name": "Jan Skrasek",
			"email": "hrach.cz@gmail.com",
			"homepage": "http://jan.skrasek.com"
		}
	],
	"require": {
		"php": ">=5.6",
		"nette/application": "~2.2",
		"nette/utils": "~2.2"
	},
	"require-dev": {
		"nette/di": "~2.2",
		"nette/tester": "~1.3",
		"tracy/tracy": "~2.2",
		"mockery/mockery": "~0.9",
		"nikic/php-parser": "~2.0"
	},
	"suggest": {
		"nette/di": "to use SecuredLinksExtension",
		"nikic/php-parser": "to detect return types without @return annotation"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.3-dev"
		}
	},
	"autoload": {
		"psr-4": { "Nextras\\SecuredLinks\\": "src/" }
	},
	"replace": {
		"nextras/application": "self.version"
	}
}
